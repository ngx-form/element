{
  "name": "@ngx-form/element",
  "version": "0.2.0",
  "description": "Angular 2+ component to dynamically create html form element using data attribute with previously configured component with element name",
  "author": "Ścibor Rudnicki <scibor.rudnicki@wwwdev.io> (http://ngx-form.wwwwdev.io)",
  "homepage": "https://github.com/ngx-form/element#readme",
  "main": "dist/bundle.umd.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "contributors": [
    "Ścibor Rudnicki <scibor.rudnicki@wwwdev.io>"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "bundle": "rollup -c rollup.config.js",
    "prebuild": "",
    "build": "./node_modules/.bin/ngc -p tsconfig.json",
    "postbuild": "npm run html && npm run bundle",
    "prepublish": "npm run build",
    "start": "npm run build",
    "html": "cd src && copyfiles **/*.html *.html *.scss **/*.scss ../dist",
    "test": "./node_modules/karma/bin/karma start karma.conf.js"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/tsc-wrapped": "^4.3.0",
    "@types/lodash-es": "^4.14.5",
    "lodash-es": "^4.17.4"
  },
  "devDependencies": {
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/compiler-cli": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/forms": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@ngx-core/common": "^1.1.1",
    "@ngx-form/interface": "^0.5.1",
    "@ngx-form/type": "^0.1.6",
    "@types/jasmine": "^2.5.53",
    "@types/karma": "^0.13.36",
    "@types/lodash": "^4.14.69",
    "@types/node": "^8.0.13",
    "codelyzer": "^3.1.2",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "jasmine": "^2.6.0",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-rollup-plugin": "^0.2.4",
    "karma-typescript": "^3.0.4",
    "lodash": "^4.17.4",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-angular": "^0.4.4",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.4.2",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1",
    "uglify-es": "^3.0.25",
    "zone.js": "^0.8.13"
  },
  "keywords": [
    "Angular",
    "Component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngx-form/element.git"
  },
  "bugs": {
    "url": "https://github.com/ngx-form/element/issues"
  }
}
